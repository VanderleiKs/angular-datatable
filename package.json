{
  "name": "angular-datatable",
  "version": "1.0.0",
  "description": "DataTable component for Angular framework 7",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "ngc -p src",
    "start": "http-server -c-1 -o -p 8875 .",
    "pretest": "npm run build",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage-remap": "remap-istanbul -i coverage/coverage-final.json -o coverage/coverage-remapped.lcov -t lcovonly",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanderleiks/angular-datatable.git"
  },
  "keywords": [
    "angular",
    "angular6",
    "ng",
    "ng2",
    "table",
    "pagination",
    "sort",
    "sorting"
  ],
  "author": "khan.abdul1@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariuszfoltak/angular2-datatable/issues"
  },
  "homepage": "https://github.com/mariuszfoltak/angular2-datatable#readme",
  "devDependencies": {
    "@angular/core": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/compiler-cli": "^7.2.16",
    "core-js": "^2.5.5",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.10.1",
    "rxjs": "^6.6.7",
    "systemjs": "^0.21.3",
    "typescript": "3.2.4",
    "zone.js": "^0.8.29",
    "rimraf": "^2.6.2",
    "@types/lodash": "4.14.108",
    "@types/jasmine": "^2.8.7"
  },
  "dependencies": {
    "@angular/cli": "^7.3.10",
    "lodash": "^4.0.0"
  },
  "peerDependencies": {}
}
